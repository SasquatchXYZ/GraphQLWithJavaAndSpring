# This is a comment

# This is a root Object, because it is named Query
type Query {
    # A field named "pets" which returns a list of Pet
    pets: [Pet]
    pet(id: ID!): Pet
}

# An object named Pet
type Pet {
    id: ID!
    name: String
    color: String
    owner: Person
}

type Person {
    name: String
}